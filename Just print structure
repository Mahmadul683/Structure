
#include<stdio.h>
struct person 
{
    int age;
    float salary;
};
int main()
{
struct person person1,person2;
person1.age=20;
person1.salary=20.500;
printf("Person1:");
printf("Age=%d",person1.age);
printf("Salary=%.2f",person1.salary);
person2.age=30;
person2.salary=250.500;
printf("Person2:");
printf("Age=%d",person2.age);
printf("Salary=%.2f",person2.salary);
}
